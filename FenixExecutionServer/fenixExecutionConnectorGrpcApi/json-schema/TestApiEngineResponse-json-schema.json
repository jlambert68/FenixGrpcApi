{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "testStepExecutionStatus": {
      "type": "object",
      "properties": {
        "statusCode": {
          "type": "integer"
        },
        "statusText": {
          "type": "string"
        }
      },
      "required": ["statusCode", "statusText"],
      "additionalProperties": false
    },
    "details": {
      "type": "string"
    },
    "responseVariables": {
      "type": "string"
    },
    "executionTimeStamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["testStepExecutionStatus", "details", "responseVariables", "executionTimeStamp"],
  "additionalProperties": false
}
